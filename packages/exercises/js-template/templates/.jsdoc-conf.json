{
    "tags": {
        "allowUnknownTags": false
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "systemName": "{{titleCase name}}",
        "copyright": "Copyright {{year}}, {{author}}",
        "template": "node_modules/tui-jsdoc-template",
        "readme": "README.md",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "name": "{{titleCase name}}",
        "footerText": "{{titleCase name}}",
        "useCollapsibles": true,
        "logo": {
            "url": "https://upload.wikimedia.org/wikipedia/commons/c/ca/1x1.png",
            "width": "1px",
            "height": "1px",
            "link": "https://{{githubName}}.github.io/{{kebabCase name}}/"
        },
        "css": [
            "jsdoc.css"
        ]
    },
    "docdash": {
        "static": true,
        "sort": true
    }
}
